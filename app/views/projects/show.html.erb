<h1><%= @project.name %></h1>
<div>
  <% @project.phases.each do |phase| %>
  <div class="panel panel-default">
    <div class="panel-heading">
      <h4><strong><%= phase.name %></strong></h4>
    </div>
    <table class="table table-hover">
      <thead>
        <tr>
          <th> Task Name </th>
          <th> Start Date </th>
          <th> End Date </th>
          <th> Due Date </th>
          <th> Status </th>
        </tr>
      </thead>
      <tbody>
        <%= render phase.tasks %>
      </tbody>
    </table>
  </div>
  <% end %>
</div>
